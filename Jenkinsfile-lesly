    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Hello2') {
            steps {
                sh ''' mkdir leslypipeline0 '''
            }
        }
        stage('Hello3') {
            steps {
                sh ''' touch newpipelineher'''
                echo 'done ...'
            }
        }
    }
}
